<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.next_app">

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/next"
        android:label="Next"
        android:roundIcon="@drawable/next"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ScheduleViewerActivity"
            android:label="@string/title_activity_schedule_viewer"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".Phone_MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Phone_MenuActivity"></activity>
    </application>

</manifest>